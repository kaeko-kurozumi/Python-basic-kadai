      "source": [
        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前: {self.name}, 年齢: {self.age}\")\n",
        "\n",
        "# Humanクラスのインスタンスを作成\n",
        "person1 = Human(\"太郎\", 25)\n",
        "\n",
        "# printinfoメソッドを使用して名前と年齢を標準出力に表示\n",
        "person1.printinfo()"
      ]
    }
  ]
