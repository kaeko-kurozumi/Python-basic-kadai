      "source": [
        "def total(price1, price2):\n",
        "    tax_rate = 0.1  # 消費税率は0.1 (10%) とする\n",
        "    subtotal = price1 + price2\n",
        "    total_with_tax = subtotal * (1 + tax_rate)\n",
        "    return total_with_tax\n",
        "\n",
        "price1 = 100\n",
        "price2 = 200\n",
        "\n",
        "result = total(price1, price2)\n",
        "print(result)\n"
      ]
    }
